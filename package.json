    "swagger-jsdoc": "1.6.0",
    "uuid": "3.1.0",
    "ws": "1.0.1"
  },
  "devDependencies": {
    "babel-polyfill": "6.23.0",
    "babel-register": "6.24.1",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "del": "2.2.0",
    "eslint": "3.4.0",
    "eslint-config-standard": "6.0.0",
    "eslint-plugin-i18n": "1.0.2",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-standard": "2.0.0",
    "gulp": "3.9.1",
    "gulp-git": "1.7.1",
    "gulp-nodemon": "2.0.7",
