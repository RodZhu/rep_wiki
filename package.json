    "mocha-jenkins-reporter": "0.3.8",
    "mocha-testcheck": "0.1.2",
    "mockery": "2.1.0",
    "nodemon": "1.10.2",
    "nyc": "11.0.3",
    "password-generator": "2.0.2",
    "promise-polyfill": "5.2.1",
    "promise-test-helper": "0.2.1",
    "redoc": "1.19.0",
    "rewire": "2.5.1",
    "sinon": "2.3.4",
    "yargs": "3.32.0"
  },
  "scripts": {
    "start": "nodemon server.js | bunyan",
    "eslint": "eslint './**/*.js' '../backend_automation/**/*.js' '../ui_automation/**/*.js'",
    "pretest": "eslint './**/*.js' '../backend_automation/**/*.js' '../ui_automation/**/*.js'",
    "test": "nyc --reporter text-summary mocha --recursive --check-leaks --throw-deprecation --colors test",
    "coverage-text": "nyc report --reporter text --reporter text-summary",
    "coverage-html": "firefox coverage/index.html &",
    "jsdoc": "jsdoc --configure jsdoc.json",
    "local_integration": "JUNIT_REPORT_PATH=../backend_automation/logs/backend_local.xml JUNIT_REPORT_STACK=1 mocha --reporter mocha-jenkins-reporter --grep @noLocal --invert ../backend_automation/setup/integration.js --recursive ../backend_automation/test",
    "ova_integration": "JUNIT_REPORT_PATH=../backend_automation/logs/backend_ova.xml JUNIT_REPORT_STACK=1 mocha --reporter mocha-jenkins-reporter --grep @noOva --invert ../backend_automation/setup/acceptance.js --recursive ../backend_automation/test",
    "scale": "mocha ../backend_automation/setup/scalePerformance.js ../backend_automation/test_scale/*Performance.js",
    "scale_ov": "mocha --require babel-polyfill --compilers js:babel-register ../backend_automation/test_scale_ov/scale_f172.js",
    "scale_hc380": "mocha ../backend_automation/setup/scaleSetupHC380.js --recursive ../backend_automation/test_scale_hc380",
    "coverage": "istanbul cover --include-all-sources _mocha -- -R spec 'test/**/*.js'"
  }
}
